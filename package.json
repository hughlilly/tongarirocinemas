{
  "name": "tongarirocinemas",
  "version": "1.0.0",
  "description": "Tongariro Cinemas website",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "format": "prettier --write \"src/**/*.{js,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,html}\"",
    "watch": "npx tailwindcss -i ./src/styles/main.css -o ./dist/output.css --watch",
    "build:tw": "npx tailwindcss -i ./src/styles/main.css -o ./dist/output.css",
    "open": "serve dist"
  },
  "keywords": [ ],
  "author": "Hugh Lilly",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
